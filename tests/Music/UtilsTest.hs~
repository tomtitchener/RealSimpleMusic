module Music.UtilsTest where

import Test.HUnit
import Test.QuickCheck

import Music.Utils

testSliceInMiddle :: Assertion
testSliceInMiddle =
  [4,5] @=? slice 4 2 [0..10]

propSliceAllIsOriginalList :: [Int] -> Property
propSliceAllIsOriginalList xs =
    not (null xs) ==>
        slice 0 (length xs) xs == xs


